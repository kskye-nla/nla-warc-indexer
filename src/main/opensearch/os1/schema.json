{
   "settings":{
      "analysis":{
         "analyzer":{
            "standard_analyzer":{
               "tokenizer":"standard_tokenizer",
               "filter":[
                  "lowercase"
               ]
            },
            "hex_text_shingle_analyzer":{
               "tokenizer":"whitespace_tokenizer",
               "filter":[
                  "hex_text_shingle_filter"
               ]
            },
            "path_analyzer":{
               "tokenizer":"whitespace_tokenizer",
               "filter":[
                  "word_delimiter_graph",
                  "path_stopwords_filter",
                  "lowercase"
               ]
            }
         },
         "tokenizer":{
            "standard_tokenizer":{
               "type":"standard"
            },
            "whitespace_tokenizer":{
               "type":"whitespace"
            }
         },
         "filter":{
            "hex_text_shingle_filter":{
               "type":"shingle",
               "min_shingle_size":4,
               "max_shingle_size":7,
               "output_unigrams":false,
               "output_unigrams_if_no_shingles":false,
               "token_separator":" "
            },
            "path_stopwords_filter":{
               "type":"stop",
               "stopwords_path":"extra/stopwords_path.txt"
            }
         }
      }
   },
